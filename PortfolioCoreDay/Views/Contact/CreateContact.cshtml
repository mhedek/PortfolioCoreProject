@model Contact
@{
    ViewData["Title"] = "CreateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Yeni İletişim Bilgisi Ekleme</h2>

<form method="post">
    <label>Adres</label>
    <input type="text" asp-for="Address" class="form-control" style="border:1px solid black" />
    <br />

    <label>Telefon</label>
    <input type="text" asp-for="Phone" class="form-control" style="border:1px solid black" />
    <br />

    <label>Mail</label>
    <input type="text" asp-for="Mail" class="form-control" style="border:1px solid black" />
    <br />

    <label>Harita Url</label>
    <input type="text" asp-for="Map" class="form-control" style="border:1px solid black" />
    <br />

    <button class="btn btn-primary">Kaydet</button>
</form>

