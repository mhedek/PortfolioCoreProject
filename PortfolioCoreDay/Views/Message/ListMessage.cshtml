@model List<Message>

@{
    ViewData["Title"] = "ListMessage";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Mesaj Listesi</h2>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Ad Soyad</th>
        <th>E Mail</th>
        <th>Konu</th>
        <th>Mesaj</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.MessageId</td>
            <td>@item.NameSurname</td>
            <td>@item.Email</td>
            <td>@item.Subject</td>
            <td>@item.MessageDetails</td>
        </tr>
    }
</table>



