@model List<Education>

<section id="resume" class="resume section">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>Eğitimler Ve Deneyimler</h2>
    </div><!-- End Section Title -->

    <div class="container">

        <div class="row">

            <div class="col-lg-6" data-aos="fade-up" data-aos-delay="100">

                <h3 class="resume-title">Eğitim Hayatım</h3>
                @foreach (var item in Model)
                {
                    <div class="resume-item">
                        <h4>@item.Title</h4>
                        <h5>@item.EducationDate</h5>
                        <p><em>@item.University</em></p>
                        <p>@item.Department</p>
                    </div>

                    <!-- Edn Resume Item -->
                }

            </div>

            @await Component.InvokeAsync("_DefaultExperienceComponentPartial")

        </div>

    </div>

</section>
