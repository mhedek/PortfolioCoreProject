    <script src="/iPortfolio-1.0.0/assets/vendor/imagesloaded/imagesloaded.pkgd.min.js"></script>
    <script src="/iPortfolio-1.0.0/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="/iPortfolio-1.0.0/assets/vendor/swiper/swiper-bundle.min.js"></script>

    <!-- Main JS File -->
    <script src="/iPortfolio-1.0.0/assets/js/main.js"></script>

