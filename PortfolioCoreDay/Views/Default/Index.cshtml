@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_DefaultHeadComponentPartial")



<body class="index-page">

    @await Component.InvokeAsync("_DefaultSidebarComponentPartial")


    <main class="main">

        <!-- Hero Section -->
        @await Component.InvokeAsync("_DefaultSliderComponentPartial")

        <!-- /Hero Section -->
        <!-- About Section -->
        @await Component.InvokeAsync("_DefaultAboutComponentPartial")
        <!-- /About Section -->
        <!-- Stats Section -->
        @await Component.InvokeAsync("_DefaultIstatistikComponentPartial")

        <!-- /Stats Section -->
        <!-- Skills Section -->
        @await Component.InvokeAsync("_DefaultSkillComponentPartial")

        <!-- /Skills Section -->
        <!-- Resume Section -->
        @await Component.InvokeAsync("_DefaultEducationComponentPartial")

        <!-- /Resume Section -->
        <!-- Portfolio Section -->
        @await Component.InvokeAsync("_DefaultPortfolioComponentPartial")

        <!-- /Portfolio Section -->
        <!-- Services Section -->
        @await Component.InvokeAsync("_DefaultServiceComponentPartial")

        <!-- /Services Section -->
        <!-- Testimonials Section -->
        @await Component.InvokeAsync("_DefaultTestimonialComponentPartial")
        <!-- /Testimonials Section -->
        <!-- Contact Section -->
        @await Component.InvokeAsync("_DefaultContactComponentPartial")


    </main>

    <footer id="footer" class="footer position-relative light-background">

        <div class="container">
            <div class="copyright text-center ">
                <p>© <span>Copyright</span> <strong class="px-1 sitename">iPortfolio</strong> <span>All Rights Reserved</span></p>
            </div>
            <div class="credits">
                <!-- All the links in the footer should remain intact. -->
                <!-- You can delete the links only if you've purchased the pro version. -->
                <!-- Licensing information: https://bootstrapmade.com/license/ -->
                <!-- Purchase the pro version with working PHP/AJAX contact form: [buy-url] -->
                Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a> Distributed by <a href="https://themewagon.com">ThemeWagon</a>
            </div>
        </div>

    </footer>

    <!-- Scroll Top -->
    <a href="#" id="scroll-top" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Preloader -->
    <div id="preloader"></div>

    <!-- Vendor JS Files -->
    <script src="/iPortfolio-1.0.0/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="/iPortfolio-1.0.0/assets/vendor/php-email-form/validate.js"></script>
    <script src="/iPortfolio-1.0.0/assets/vendor/aos/aos.js"></script>
